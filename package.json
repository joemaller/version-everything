{
  "name": "version-everything",
  "version": "0.9.1",
  "description": "Use npm to version all kinds of projects, not just javascript.",
  "keywords": [
    "npm",
    "semver",
    "version"
  ],
  "type": "module",
  "homepage": "https://github.com/joemaller/version-everything#readme",
  "bugs": {
    "url": "https://github.com/joemaller/version-everything/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joemaller/version-everything.git"
  },
  "license": "MIT",
  "author": "Joe Maller <joe@joemaller.com> (http://www.joemaller.com)",
  "main": "index.js",
  "bin": {
    "version-everything": "cli.js"
  },
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:debug": "node --inspect-brk $(npm bin)/jest --runInBand",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "version": "node cli.js README.md && auto-changelog && git add -u"
  },
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "node_modules",
      "__snapshots__"
    ],
    "snapshotSerializers": [
      "jest-snapshot-serializer-raw"
    ],
    "testTimeout": 500,
    "verbose": true,
    "moduleNameMapper": {
      "#(.*)": "<rootDir>/node_modules/$1"
    }
  },
  "dependencies": {
    "auto-changelog": "^2.4.0",
    "chalk": "^5.0.1",
    "cosmiconfig": "^7.0.1",
    "escape-string-regexp": "^5.0.0",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "lodash.isplainobject": "^4.0.6",
    "plist": "^3.0.6",
    "read-pkg-up": "^9.1.0",
    "semver": "^7.3.7",
    "sort-package-json": "^1.57.0",
    "universalify": "^2.0.0",
    "xml-js": "^1.6.11",
    "yaml": "^2.1.1",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/semver": "^7.3.12",
    "jest": "^29.0.3",
    "jest-snapshot-serializer-raw": "^1.2.0",
    "tmp": "0.2.1"
  },
  "engines": {
    "node": ">=14"
  },
  "auto-changelog": {
    "package": true,
    "backfillLimit": 7
  }
}
